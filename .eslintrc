{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": [
    "uber-es2015",
    "uber-jsx",
    "plugin:import/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "import/unambiguous": 0,
    "consistent-return": 0,
    "max-params": ["error", 6]
  },
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "settings": {
    "import/core-modules": [
      "react-vis"
    ]
  }
}
