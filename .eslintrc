{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "uber-es2015",
    "uber-jsx",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "consistent-return": 0,
    "max-len": [1, 110, 4],
    "max-params": ["error", 6],
    "object-curly-spacing": 0,
    "babel/object-curly-spacing": 2,
    "jsx-a11y/no-static-element-interactions": 0
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel",
    "jsx-a11y"
  ]
}
