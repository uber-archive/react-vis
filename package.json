{
  "name": "react-vis",
  "version": "0.9.0",
  "license": "MIT",
  "author": "Anton Bulyenov <antonb@uber.com>",
  "description": "Data visualization library based on React and d3.",
  "keywords": [
    "d3",
    "react",
    "visualization",
    "chart",
    "es6",
    "babel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-common/react-vis.git"
  },
  "bugs": {
    "url": "https://github.com/uber-common/react-vis/issues/new",
    "email": "antonb@uber.com"
  },
  "main": "index.js",
  "dependencies": {
    "d3-array": "^1.0.2",
    "d3-collection": "^1.0.2",
    "d3-color": "^1.0.2",
    "d3-hierarchy": "^1.0.3",
    "d3-interpolate": "^1.1.2",
    "d3-scale": "^1.0.4",
    "d3-shape": "^1.0.4",
    "deep-equal": "^1.0.1",
    "global": "^4.3.1",
    "react-motion": "^0.4.7",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "brfs": "^1.4.3",
    "browserify": "^13.3.0",
    "buble-loader": "^0.4.0",
    "budo": "^9.4.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-uber-es2015": "^3.0.1",
    "eslint-config-uber-jsx": "^3.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "node-sass": "^4.2.0",
    "react": ">=15.4.2",
    "react-addons-shallow-compare": ">=15.4.2",
    "react-addons-test-utils": ">=15.4.2",
    "react-dom": ">=15.4.2",
    "run-browser-babel": "^5.2.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "tape": "^4.6.3",
    "uglify-js": "^2.7.5",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "peerDependencies": {
    "react": "0.14.x - 15.x",
    "react-addons-shallow-compare": "0.14.x - 15.x"
  },
  "scripts": {
    "build-js": "babel src -d dist --copy-files",
    "build-docs": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config ./webpack.config.examples.js --progress --profile --colors --output-filename examples.js",
    "build-css": "node-sass src/main.scss main.css",
    "watch": "webpack-dev-server --content-base examples --config ./webpack.config.examples.js --progress --hot --port 3001 --open",
    "build": "npm run build-js && npm run build-css",
    "lint": "eslint --cache --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty .",
    "execute-tests": "run-browser-babel dist/test/index.js --bx react/addons --bx react/lib/ReactContext --bx react/lib/ExecutionEnvironment --phantom",
    "test": "npm run build-js && npm run execute-tests",
    "cover": "npm run build-js && npm run execute-tests -- --report=text --report=cobertura --report=html --report=json-summary",
    "precommit": "npm run lint -s",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=3.0"
  }
}
