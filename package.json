{
  "name": "react-vis-master",
  "version": "1.11.7",
  "license": "MIT",
  "author": "Visualization Team <visualization@uber.com>",
  "description": "Data visualization library based on React and d3.",
  "private": true,
  "workspaces": [
    "packages/showcase",
    "packages/react-vis",
    "packages/website"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uber-common/react-vis.git"
  },
  "bugs": {
    "url": "https://github.com/uber-common/react-vis/issues/new",
    "email": "visualization@uber.com"
  },
  "keywords": [
    "d3",
    "react",
    "visualization",
    "chart",
    "es6",
    "babel"
  ],
  "scripts": {
    "remove-unpm-rfs": "./remove-refs-to-unpm.pl"
  },
  "devDependencies": {
    "husky": "^1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run remove-unpm-rfs && git add yarn.lock"
    }
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ]
        }
      ]
    ],
    "env": {
      "production": {
        "presets": [
          "es2015",
          "stage-0",
          "react"
        ]
      },
      "development": {
        "presets": [
          "es2015",
          "stage-0",
          "react"
        ]
      },
      "es": {
        "presets": [
          [
            "es2015",
            {
              "modules": false
            }
          ],
          "stage-0",
          "react"
        ]
      }
    }
  },
  "volta": {
    "node": "10.20.1",
    "yarn": "1.22.4"
  },
  "resolutions": {
    "js-beautify": "1.10.3"
  }
}
